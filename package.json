  "build": {
    "appId": "com.gamingplatform.app",
    "productName": "Gaming Platform",
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*",
      "electron.js"
    ],
    "mac": {
      "category": "public.app-category.games",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
